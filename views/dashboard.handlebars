<br>
<h2>DASHBOARD</h2>
<br>
<h3>This is your home to create and share whats going on in that mind of yours</h3>
<br>



{{#each posts as |post| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <a href="/post/{{post.id}}">{{post.name}}</a>
    </h2>
  </div>
  <div class="col-md-7">
    <p>
      {{post.description}}
    </p>
  </div>
</div>
{{/each}}
<div>
<input id="post-title"  placeholder="Title of your post"></input><br>
<input id="post-desc"  placeholder="Description"></input><br>
<br>
<button style="background-color:darkorange id="newPost">Post</button>
</div>
<script src="./js/profile.js"></script>
